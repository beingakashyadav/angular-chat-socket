{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "**/*",
      "headers": [
        {"key": "X-Frame-Options", "value": "ALLOW-FROM https://www.facebook.com"}
      ]
    }]
  }
}
